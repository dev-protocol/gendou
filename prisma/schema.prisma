// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model send_info {
  id      				Int      @default(autoincrement()) @id
  github_id   		String   @unique
  reward    			String
  uuid    				String   @unique
  claim_url 	   	String
  find_at 				DateTime?
}
