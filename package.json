{
	"name": "template-repos-ts-functions",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"test": "ava --serial",
		"build": "tsc",
		"watch": "tsc -w",
		"start": "func start",
		"pretest": "yarn build",
