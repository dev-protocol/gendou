		"dotenv": "10.0.0",
		"eslint": "7.29.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-functional": "3.2.1",
		"eslint-plugin-prettier": "3.4.0",
		"husky": "7.0.0",
		"prettier": "2.3.2",
		"rimraf": "3.0.2",
		"sinon": "11.1.1",
		"ts-node": "10.0.0",
		"typescript": "4.3.5"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			}
		}
