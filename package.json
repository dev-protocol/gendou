		"@types/bignumber.js": "5.0.0",
		"@types/mathjs": "9.4.1",
		"bignumber.js": "9.0.1",
		"ethereum-waffle": "3.4.0",
		"mathjs": "9.4.3",
		"prisma": "2.26.0",
		"ramda": "0.27.1"
	},
	"devDependencies": {
		"@ava/typescript": "1.1.1",
		"@types/bent": "7.3.2",
		"@types/deep-equal": "1.0.1",
		"@types/dotenv": "8.2.0",
		"@types/node": "14.17.4",
		"@types/ramda": "0.27.41",
		"@types/sinon": "10.0.2",
		"@typescript-eslint/eslint-plugin": "4.28.1",
		"@typescript-eslint/parser": "4.28.1",
