	"dependencies": {
		"@azure/functions": "1.2.3",
		"@devprotocol/util-ts": "2.2.1",
		"@prisma/client": "2.19.0",
		"@types/bignumber.js": "5.0.0",
		"bent": "7.3.12",
		"bignumber.js": "9.0.1",
		"ethereum-waffle": "3.3.0",
		"prisma": "2.19.0",
		"ramda": "0.27.1"
	},
	"devDependencies": {
		"@ava/typescript": "1.1.1",
		"@types/bent": "7.3.2",
		"@types/dotenv": "8.2.0",
		"@types/node": "14.14.37",
		"@types/ramda": "0.27.39",
		"@types/sinon": "9.0.11",
		"@types/deep-equal": "1.0.1",
		"@typescript-eslint/eslint-plugin": "4.20.0",
		"@typescript-eslint/parser": "4.20.0",
		"ava": "3.15.0",
		"dotenv": "8.2.0",
		"eslint": "7.23.0",
		"eslint-config-prettier": "6.15.0",
		"eslint-plugin-functional": "3.2.1",
		"eslint-plugin-prettier": "3.3.1",
		"husky": "6.0.0",
		"prettier": "2.2.1",
		"rimraf": "3.0.2",
		"sinon": "10.0.0",
		"deep-equal": "2.0.5",
		"ts-node": "9.1.1",
		"typescript": "4.2.3"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			}
		}
	}
