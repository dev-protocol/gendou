	},
	"dependencies": {
		"@azure/functions": "3.0.0",
		"@devprotocol/util-ts": "2.2.1",
		"@octokit/graphql": "4.8.0",
		"@prisma/client": "3.11.1",
		"@types/bignumber.js": "5.0.0",
		"@types/mathjs": "9.4.1",
		"bignumber.js": "9.0.2",
		"ethereum-waffle": "3.4.4",
		"mathjs": "10.4.1",
		"prisma": "3.11.1",
		"ramda": "0.28.0"
	},
	"devDependencies": {
		"@ava/typescript": "1.1.1",
		"@types/bent": "7.3.2",
		"@types/deep-equal": "1.0.1",
		"@types/dotenv": "8.2.0",
		"@types/node": "16.11.26",
		"@types/ramda": "0.28.4",
		"@types/sinon": "10.0.11",
		"@typescript-eslint/eslint-plugin": "5.17.0",
		"@typescript-eslint/parser": "5.17.0",
		"ava": "4.1.0",
		"deep-equal": "2.0.5",
		"dotenv": "16.0.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-functional": "3.7.2",
		"eslint-plugin-prettier": "4.0.0",
		"husky": "7.0.4",
		"prettier": "2.6.1",
		"rimraf": "3.0.2",
		"sinon": "13.0.1",
		"ts-node": "10.7.0",
		"typescript": "4.6.3"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			}
		}
